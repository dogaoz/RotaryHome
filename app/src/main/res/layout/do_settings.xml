<RelativeLayout
     xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/SettingsRelative"
    tools:context=".Settings"
   android:background="#FFFFFF">


  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton3"
      android:layout_width="80dp"
      android:layout_height="80dp"
                 android:layout_marginRight="15dp"

      android:layout_alignParentRight="true"
      android:layout_alignTop="@+id/circleButton1"
      android:src="@drawable/weather" />

  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton2"
      android:layout_width="80dp"
      android:layout_height="80dp"
            android:layout_alignTop="@+id/circleButton1"
      android:layout_centerHorizontal="true"
      android:src="@drawable/personalization" />

  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton6"
      android:layout_width="80dp"
      android:layout_height="80dp"
      android:layout_alignParentRight="true"
      android:layout_alignTop="@+id/circleButton4"
      android:src="@drawable/about"
           android:layout_marginRight="15dp"
/>

  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton5"
      android:layout_width="80dp"
      android:layout_height="80dp"
            android:layout_alignTop="@+id/circleButton4"
      android:layout_centerHorizontal="true"
      android:src="@drawable/homescreen"/>


  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton4"
      android:layout_width="80dp"
      android:layout_height="80dp"
           android:layout_marginLeft="15dp"
      android:layout_alignParentLeft="true"
      android:layout_centerVertical="true"
      android:src="@drawable/more" />


  <at.markushi.ui.CircleButton
      android:id="@+id/circleButton1"
      android:layout_width="80dp"
      android:layout_height="80dp"
      android:layout_marginLeft="15dp"
      android:layout_above="@+id/circleButton4"
      android:layout_alignParentLeft="true"
      android:layout_marginBottom="45dp"
      android:src="@drawable/onlinecontent" />
  
  
  <TextView
      android:id="@+id/textView1"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_above="@+id/circleButton4"
      android:layout_alignLeft="@+id/circleButton4"
      android:layout_alignRight="@+id/circleButton1"
      android:layout_below="@+id/circleButton2"
      android:gravity="center"
      android:text="@string/online_content"
      android:textSize="14sp" />

   <TextView
       android:id="@+id/textView2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_above="@+id/circleButton4"
       android:layout_below="@+id/circleButton2"
       android:layout_toLeftOf="@+id/textView3"
       android:layout_toRightOf="@+id/textView1"
       android:gravity="center"
       android:text="@string/personalization"
       android:textSize="14sp" />

 <TextView
     android:id="@+id/textView3"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_above="@+id/circleButton4"
     android:layout_alignLeft="@+id/circleButton3"
     android:layout_alignRight="@+id/circleButton3"
     android:layout_below="@+id/circleButton2"
     android:gravity="center"
     android:text="@string/weather"
     android:textSize="14sp" />

 <TextView
     android:id="@+id/textView4"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:layout_alignLeft="@+id/circleButton4"
     android:layout_alignRight="@+id/circleButton4"
     android:layout_below="@+id/circleButton4"
     android:gravity="center"
     android:text="@string/more"
     android:textSize="14sp" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_alignBottom="@+id/textView4"
      android:layout_alignLeft="@+id/textView2"
      android:layout_alignRight="@+id/textView2"
      android:layout_below="@+id/circleButton4"
      android:gravity="center"
      android:text="@string/myaccount"
      android:textSize="14sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/circleButton6"
        android:layout_alignRight="@+id/circleButton6"
        android:layout_below="@+id/circleButton4"
        android:gravity="center"
        android:text="@string/about"
        android:textSize="14sp" />

</RelativeLayout>
